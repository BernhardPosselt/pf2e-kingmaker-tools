flags":{}},{"type":0,"weight":1,"range":[18,19],"drawn":false,"documentCollection":"","_id":"FqvvuAqyo2NcVBVQ","text":"1 @UUID[Compendium.pf2e.pathfinder-bestiary.gvCCATlH9mPGWbsp]{Troll} (Moderate 3)","img":"icons/svg/d20-black.svg","documentId":null,"flags":{}},{"type":0,"weight":1,"range":[20,20],"drawn":false,"documentCollection":"","_id":"GVUH0GWZEzBae3gI","text":"1 @UUID[Compendium.pf2e.pathfinder-bestiary.6XlGTt3RveX49YbC]{Shambler} (Severe 3)","img":"icons/svg/d20-black.svg","documentId":null,"flags":{}}],"formula":"1d20","replacement":true,"displayRoll":true,"ownership":{"default":0,"oxLvfZiIFxns2JLx":3},"flags":{"exportSource":{"world":"kingmaker","system":"pf2e","coreVersion":"10.291","systemVersion":"4.4.2"},"core":{"sourceId":"RollTable.7SHiJIk5XpO6fPfP"}},"_stats":{"systemId":"pf2e","systemVersion":"4.4.2","coreVersion":"10.291","createdTime":1663289509438,"modifiedTime":1670817700688,"lastModifiedBy":"oxLvfZiIFxns2JLx"},"folder":null,"sort":0,"_id":"4QB0PrtFdXZJWd5x"}
